{
    "name": "jeroenvdheuve/assetic-cache-busting-worker-bundle",
    "description": "Assetic cache busting worker bundle. Uses the assetic cache busting worker to add a hash to the filename based on file content.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "assetic",
        "cache",
        "busting",
        "bundle",
        "hash",
        "content",
        "modification time"
    ],
    "authors": [
        {
            "name": "Jeroen van den Heuvel",
            "email": "jeroen.van.den.heuvel@werkspot.nl"
        }
    ],
    "require": {
        "jeroenvdheuve/assetic-cache-busting-worker": "~1.0",
        "symfony/config": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/http-kernel": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0"

    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": { "jvdh\\AsseticCacheBustingBundle\\": "" }
    },
    "minimum-stability": "stable"
}
